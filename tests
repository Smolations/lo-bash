#!/bin/bash

source SOURCEME


function h1 {
  echo -n "`printf '\n ---==:  %-15s  ' $1`"
}
function pass {
  echo -n '.'
}
function fail {
  local txt='X'
  [[ -z "$@" ]] || txt+="[$@]"
  echo -n "$txt"
}
function skip {
  echo -n 'S'
}
export -f pass fail skip h1


# source foo.sh


